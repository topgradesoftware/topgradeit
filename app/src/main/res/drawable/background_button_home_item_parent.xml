<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <solid android:color="#693e02" /> <!-- Dark brown color for parent -->
    <corners android:radius="10dp" /> <!-- Rounded corners, adjust the radius as needed -->
    <padding
        android:left="8dp"
        android:top="8dp"
        android:right="8dp"
        android:bottom="8dp" /> <!-- Padding for slight shadow effect -->
